@{
    Layout = null;
}
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title></title>
	<link href='https://fonts.googleapis.com/css?family=Lato:300,400|Montserrat:700' rel='stylesheet' type='text/css'>
	<link href='//cdnjs.cloudflare.com/ajax/libs/normalize/3.0.1/normalize.min.css' rel='stylesheet' type='text/css'>
	<link href='//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css' rel='stylesheet' type='text/css'>
	<link rel="stylesheet" href="/css/default_thank_you.css">
	<link rel="stylesheet" href="/css/site.css">
	<script src="/lib/jquery/dist/jquery.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/ark-js-util@latest/ark-util.js"></script>
</head>
<body>
	<header class="site-header" id="header">
		<h1 class="site-header__title" data-lead-id="site-header-title">BOOKING STATUS!</h1>
	</header>

	<div class="main-content">
		<p class="main-content__body" data-lead-id="main-content-body">
			Total Bookings: <span>@ViewBag.BookingCount</span> <br /><br />
			Total Count (w/ Accompanying): <span>@ViewBag.TotalCount</span> <br /><br />
		</p>
	</div>

	<div>
		<table class="rwd-table">
			<tr>
				<th>Record Id</th>
				<th>Name</th>
				<th>Phone</th>
				<th>Profession</th>
				<th>Fellowship</th>
				<th>Count</th>
				<th>Ip</th>
				<th>At</th>
			</tr>
			@foreach(var v in ViewBag.List)
			{
			<tr>
				<td data-th="Record Id">@v.record_id</td>
				<td data-th="Name">@v.full_name</td>
					<td data-th="Phone">@v.email_phone</td>
					<td data-th="Profession">@v.profession</td>
					<td data-th="Fellowship">@v.fellowship_src</td>
					<td data-th="Count">@v.accomponied</td>
					<td data-th="Ip">@v.ip</td>
					<td class="u-at" data-th="At">@v.at</td>
			</tr>
			}
		</table>
	</div>

	<footer class="site-footer" id="footer">
	</footer>
	<script>
		(function ($) {
			$("td.u-at").each((i, t) => {
				t.innerText = util.dater.toLocalTime(new Date(t.innerText));
            });
		})(jQuery);
	</script>
</body>
</html>